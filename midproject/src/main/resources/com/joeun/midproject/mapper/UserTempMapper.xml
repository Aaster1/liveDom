<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- namespace="매퍼 인터페이스 경로" -->
<mapper namespace="com.joeun.midproject.mapper.UserTempMapper">

   <insert id="insert">

    INSERT INTO `user` (username,password,name,nickname,auth,email,phone,enabled,upd_date) VALUES (#{username},#{password},#{name},#{nickname},#{auth},#{email},#{phone},0,now())
   
   </insert>

   <select id="read" resultType="users">
    SELECt * FROM `user` WHERE username = #{username}
   </select>

</mapper>